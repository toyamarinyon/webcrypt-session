{
  "name": "webcrypt-session",
  "version": "0.0.1",
  "description": "WebCrypt stateless session utility using signed and encrypted cookies to store data.",
  "main": "dist/index.js",
  "scripts": {
    "build": "node script/build.js",
    "build:type": "tsc src/index.ts --emitDeclarationOnly --declaration --outDir dist",
    "test": "vitest"
  },
  "exports":{
    ".": {
      "import": "./dist/webCryptSession.mjs",
      "require": "./dist/webCryptSession.js",
      "type": "./dist/webCryptSession.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/toyamarinyon/webcrypt-session.git"
  },
  "author": "@toyamarinyon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/toyamarinyon/webcrypt-session/issues"
  },
  "homepage": "https://github.com/toyamarinyon/webcrypt-session#readme",
  "dependencies": {
    "cookie": "^0.5.0",
    "esbuild": "^0.14.42",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.11.0",
    "@types/cookie": "^0.5.1",
    "@types/node": "^17.0.40",
    "miniflare": "^2.5.0",
    "typescript": "^4.7.3",
    "vitest": "^0.13.1"
  }
}
